        <locator type="id" timeOut="5" value="card_year">ComboboxYear</locator>
        <locator type="id" timeOut="5" value="cvv2">SecureCode</locator>
        <locator type="id" timeOut="5" value="paybutton">PayNow</locator>
	</page>

