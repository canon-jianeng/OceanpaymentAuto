<?xml version="1.0" encoding="UTF-8"?>

<map>
	<!-- 三方支付域名页面
    /web/securepay, /web/standardpay, /web/fastpay, /web/billingpay, /mobile/standardpay, /web/checkoutpay,
    /web/billingpaywc, /web/standardpaywc, /web/fastpaywc, /web/fastpaylogo, /web/quickpay, /mobile/standardpaywc,
    /mobile/quickpay, /web/billingtcpay, /web/billingsecurecheckoutpay, /web/securecheckoutpay,
