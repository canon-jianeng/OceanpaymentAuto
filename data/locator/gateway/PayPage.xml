	<page pageName="paypage">
		<!--Locator lists -->
		<locator type="id" timeOut="5" value="card_number">CardNumber</locator>
		<!-- 下拉框 -->
		<locator type="id" timeOut="5" value="card_month">ComboboxMonth</locator>
