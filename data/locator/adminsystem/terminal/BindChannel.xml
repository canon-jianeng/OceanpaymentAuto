<?xml version="1.0" encoding="UTF-8"?>

<!-- 管理后台 终端号管理 -> 查询终端号通道绑定 -->
<map>
	<!-- 左侧导航栏 -->
	<page pageName="leftside">
        <!-- 查询终端号通道绑定 -->
		<locator type="id" timeOut="5" value="divclick_31">Channel</locator>
	</page>

    <!-- 查询页面 -->
    <page pageName="query">
		<!-- 终端号 -->
		<locator type="id" timeOut="5" value="gatewayForm_gwno">Terminal</locator>
        <!-- 查询按钮 -->
		<locator type="id" timeOut="5" value="queryBtn">QueryBtn</locator>
        <!-- 复选框 -->
        <locator type="id" timeOut="5" value="mids">Mids</locator>
        <!-- 新增按钮 -->
        <locator type="xpath" timeOut="5" value="//*[@id='buttonListDiv']/span[6]/a">Create</locator>
	</page>

    <!-- 新增页面 -->
    <page pageName="create">
		<!-- 账户  -->
		<locator type="id" timeOut="5" value="mcMerNo">Account</locator>
        <!-- 终端号 -->
		<locator type="id" timeOut="5" value="mrGwNo">Terminal</locator>
        <!-- 支付方式 -->
        <locator type="id" timeOut="5" value="paymethod">Paymethod</locator>
        <!-- 卡种 -->
        <locator type="xpath" timeOut="5"
            value="//*[@id='gatewayForm']//button[@id]">BusType</locator>
        <locator type="xpath" timeOut="5"
            value="//span[contains(text(), '0{0}')]/parent::label">BusTypeItem</locator>
        <!-- 银行 -->
        <locator type="id" timeOut="5" value="mcBankCode">BankCode</locator>
        <!-- 通道 -->
        <locator type="id" timeOut="5" value="mcChaCode">ChannelCode</locator>
        <!-- 支付类型 -->
        <locator type="id" timeOut="5" value="mcPaytype">PayType</locator>
        <!-- 是否开通全预授权 -->
        <locator type="id" timeOut="5" value="mcOpenAuthor">OpenAuthor</locator>
        <!-- 是否自动撤销授权 -->
        <locator type="id" timeOut="5" value="mcAutoAuthor">AutoAuthor</locator>
        <!-- 绑定状态 -->
        <locator type="id" timeOut="5" value="mcStatus">McStatus</locator>
        <!-- 提交按钮 -->
        <locator type="id" timeOut="5" value="btnAdd">SubmitBtn</locator>
	</page>

</map>
