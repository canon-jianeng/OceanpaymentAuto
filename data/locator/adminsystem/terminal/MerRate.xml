<?xml version="1.0" encoding="UTF-8"?>

<!-- 管理后台 终端号管理 -> 扣率管理 -->
<map>
    <!-- 查询页面 -->
    <page pageName="query">
		<!-- 终端号 -->
		<locator type="id" timeOut="5" value="merRateForm_mrGwNo">Terminal</locator>
        <!-- 卡种下拉框 -->
		<locator type="xpath" timeOut="5"
                 value="//*[@id='queryTable']/tbody/tr[3]/td[2]/div/span/button">CardBtn</locator>
        <locator type="xpath" timeOut="5"
                 value="//*[@id='queryTable']/tbody/tr[3]/td[2]/div/span/div/div[1]/div[1]/button[2]">AllNot</locator>
        <locator type="xpath" timeOut="5"
                 value="//*[@id='queryTable']/tbody/tr[3]/td[2]/div/span/div/div[2]/div/div/label/span">CardType</locator>
		<!-- 通道下拉框 -->
		<locator type="xpath" timeOut="5"
                 value="//*[@id='queryTable']/tbody/tr[2]/td[4]/div/span/button">ChannelBtn</locator>
        <locator type="xpath" timeOut="5"
                 value="//*[@id='queryTable']/tbody/tr[2]/td[4]/div/span/div/div[2]/div/div/label/span">Channel</locator>
        <!-- 新增 -->
		<locator type="xpath" timeOut="5" value="//*[@id='buttonListDiv']/span[6]/a">Create</locator>
        <!-- 查询结果为空 -->
		<locator type="xpath" timeOut="1"
                 value="//*[@id='merRateForm']/div[3]/div[2]/div[5]/span">ResNull</locator>
	</page>

    <!-- 新增页面 -->
    <page pageName="create">
		<!-- 账户 -->
		<locator type="id" timeOut="5" value="mrMerNo">MerNo</locator>
		<!-- 终端号 -->
		<locator type="id" timeOut="5" value="mrGwNo">Terminal</locator>
		<!-- 支付方式 -->
		<locator type="id" timeOut="5" value="paymethod">PayMethod</locator>
        <!-- 卡种 -->
        <locator type="xpath" timeOut="5"
                 value="//*[@id='merRateForm']/div[3]/div[3]/table/tbody/tr[4]/td[2]/div/span/button">CardBtn</locator>
		<locator type="xpath" timeOut="5"
                 value="//*[@id='merRateForm']/div[3]/div[3]/table/tbody/tr[4]/td[2]/div/span/div/div[2]/div/div/label/span">CardTotal</locator>
        <!-- 银行代码 -->
		<locator type="xpath" timeOut="5"
                 value="//*[@id='merRateForm']/div[3]/div[3]/table/tbody/tr[6]/td[2]/div/input">BankBtn</locator>
        <locator type="xpath" timeOut="5"
                 value="//*[@id='merRateForm']/div[3]/div[3]/table/tbody/tr[6]/td[2]/div/ul/li[text()='{0[0]}']">BankItem</locator>
        <!-- 通道 -->
		<locator type="xpath" timeOut="5"
                 value="//*[@id='merRateForm']/div[3]/div[3]/table/tbody/tr[7]/td[2]/div/input">Channelbtn</locator>
        <locator type="xpath" timeOut="5"
                 value="//*[@id='merRateForm']/div[3]/div[3]/table/tbody/tr[7]/td[2]/div/ul/li[contains(text(), '{0[0]}')]">ChannelItem</locator>
        <!-- 交易扣率 -->
		<locator type="id" timeOut="5" value="mrTradeRate">TradeRate</locator>
	</page>

</map>
